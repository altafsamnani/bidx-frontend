			<content>[bidx app="memberprofile" view="list"]</content>
		</post> 
		<post>
			<post_id>2</post_id>
			<name>member</name>
			<title>Memberprofile</title>
			<mapping>^member/([^/]*)/?</mapping>
			<content>[bidx app="memberprofile"]</content>
		</post>	
		<post>
			<post_id>3</post_id>
			<name>search</name>
			<title>Search</title>
			<mapping>^search/?</mapping>
			<content>[bidx app="search"]</content>
		</post>
		<post>
			<post_id>4</post_id>
			<name>businesslist</name>
			<title>Business List</title>
			<mapping>^business/?</mapping>
			<content>[bidx app="business" view="list"]</content>
		</post>	
		<post>
			<post_id>5</post_id>
			<name>business</name>
