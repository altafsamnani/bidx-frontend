			<mapping>^business/?</mapping>
			<content>[bidx app="businessplan" view="list"]</content>
		</post>
 -->
		<post>
			<post_id>7</post_id>
			<name>business</name>
			<title>Business</title>
			<mapping>^business/([^/]*)/?</mapping>
			<content>[bidx app="businessplan"]</content>
		</post>
		<post>
			<post_id>8</post_id>
			<name>last_members</name>
